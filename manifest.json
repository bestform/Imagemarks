{
  "name": "Imagemarks",
  "version": "1.1",
  "description": "Shows all of your image bookmarks as a list",
  "permissions": [
    "bookmarks"
  ],
  "browser_action": {
      "default_title": "Imagemarks",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
